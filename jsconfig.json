{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@assets/*": ["src/assets/*"],
            "@components/*": ["src/components/*"],
            "@configs/*": ["src/configs/*"],
            "@context/*": ["src/context/*"],
            "@data/*": ["src/data/*"],
            "@i18n/*": ["src/i18n/*"],
            "@screens/*": ["src/screens/*"],
            "@redux/*": ["src/redux/*"],
            "@repository/*": ["src/repository/*"],
            "@routes/*": ["src/routes/*"],
            "@styles/*": ["src/styles/*"],
            "@utils/*": ["src/utils/*"],
            "@location/*": ["src/location/*"]
        }
    },
    "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
